{

"manifest_version":3,
"name": "EAN GTIN Generator (local)", 
"version": "0.1.0",
"action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "icons/icon128.png",
        "32": "icons/icon128.png",
        "48": "icons/icon128.png",
        "128": "icons/icon128.png"
    }
},
"background": {
    "service_worker": "background.js",
    "type": "module"
},
"permissions": [
     "storage",
    "clipboardWrite"
],
"icons": {
       "16": "icons/icon128.png",
        "32": "icons/icon128.png",
        "48": "icons/icon128.png",
        "128": "icons/icon128.png"
}
}